<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NaoPodeAdocaoAlemDoMesAtual</fullName>
    <active>true</active>
    <errorConditionFormula>MONTH(DataAdocao__c) &gt; MONTH(TODAY())</errorConditionFormula>
    <errorMessage>Não é possível colocar Data além do Mês atual!</errorMessage>
</ValidationRule>
